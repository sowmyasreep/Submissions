@model IEnumerable<Question_1.Models.Customer>

@{ ViewBag.Title = "German Customers"; }

<h2>German Customers</h2>

<table class="table">
    <tr>
        <th>Customer ID</th>
        <th>Contact Name</th>
        <!-- Add other columns as needed -->
    </tr>
    @foreach (var customer in Model)
    {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.ContactName</td>
    <!-- Add other columns as needed -->
</tr>}
</table>
